@model List<Model>

@{
    ViewData["Title"] = "Index";
}

<h2>Models</h2>

<div class="list-group">
    <ul class="list-group">
        @foreach (var modell in Model)
        {
            <a asp-controller="Car" asp-action="Index" asp-route-id="@modell.Id" class="list-group-item list-group-item-action">@modell.Name</a>
        }
    </ul>
    <a asp-controller="Marka" asp-action="Index" class="list-group-item list-group-item-action text-danger">Back</a>
</div>