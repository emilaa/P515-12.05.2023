@model List<Marka>

@{
    ViewData["Title"] = "Index";
}

<h2>Markas</h2>

<div class="list-group">
    <ul class="list-group">
        @foreach (var marka in Model)
        {
            <a asp-controller="Model" asp-action="Index" asp-route-id="@marka.Id" class="list-group-item list-group-item-action">@marka.Name</a>
        }
    </ul>
</div>